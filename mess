SELECT (FLOOR(DATEDIFF(NOW(),birth_date)/365) AS age FROM users WHERE id = 105);
105 = user_id

SELECT user_name, FLOOR(DATEDIFF(NOW(),birth_date)/365) AS age FROM users WHERE ABS(FLOOR(DATEDIFF(NOW(),birth_date)/365) - (SELECT FLOOR(DATEDIFF(NOW(),birth_date)/365) FROM users WHERE id = 105)) < 5;

(SELECT id FROM users WHERE ABS(FLOOR(DATEDIFF(NOW(),birth_date)/365) - (SELECT FLOOR(DATEDIFF(NOW(),birth_date)/365) FROM users WHERE id = ${id})) < ${x});
